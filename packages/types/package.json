{
  "name": "@packages/types",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "description": "公共类型",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "g": "plop --plopfile plopfile.mjs \"type\"",
    "g:eslint": "eslint",
    "g:format": "prettier",
    "ts:init": "tsc --init",
    "build": "rollup -c",
    "preinstall": "pnpm build",
    "format:fix": "prettier --write \"src/**/*.{ts,cjs,mjs,js,ejs,md,html,css,scss,md,json,vue}\"",
    "format": "prettier --check \"src/**/*.{ts,cjs,mjs,js,ejs,md,html,css,scss,md,json,vue}\"",
    "eslint:fix": "eslint \"src/**/*.{ts,cjs,mjs,js,vue}\" --fix",
    "eslint": "eslint \"src/**/*.{ts,cjs,mjs,js,vue}\"",
    "lint:fix": "pnpm format:fix && pnpm eslint:fix",
    "lint": "pnpm format && pnpm eslint",
    "clear:modules": "if exist node_modules (rmdir /s /q node_modules)",
    "clear:dist": "if exist dist (rmdir /s /q dist)",
    "clear:cache": "if exist .rollup.cache (rmdir /s /q .rollup.cache)",
    "clear": "pnpm clear:dist && pnpm clear:cache && pnpm clear:modules"
  },
  "devDependencies": {
    "@configs/eslint-config": "workspace:*",
    "@packages/generators": "workspace:*",
    "@rollup/plugin-commonjs": "catalog:cli",
    "@rollup/plugin-node-resolve": "catalog:cli",
    "@rollup/plugin-typescript": "catalog:cli",
    "@types/node": "catalog:cli",
    "eslint": "catalog:cli",
    "plop": "catalog:cli",
    "rollup": "catalog:cli",
    "rollup-plugin-delete": "catalog:cli",
    "rollup-plugin-dts": "catalog:cli",
    "typescript": "catalog:cli"
  }
}
